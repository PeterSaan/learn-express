{% extends "partials/layout.njk" %}
{% block title %}Posts{% endblock %}
{% block content %}
<table class="table table-hover">
    <thead>
        <th>ID</th>
        <th>Title</th>
        <th>Created At</th>
        <th>Updated At</th>
    </thead>
    <tbody>
        <tr>
            <td>{{post.id}}</td>
            <td>{{post.title}}</td>
            <td>{{post.createdAt}}</td>
            <td>{{post.updatedAt}}</td>
        </tr>

    </tbody>
</table>
{% endblock %}